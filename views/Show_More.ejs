<html>
<head>
	<title>Kvantum</title>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
	<link rel="stylesheet" href="/css/books.css">
</head>
<body class="b2">
	<h1>Details</h1> 
	

	<div class="container">
		<% searchResults.forEach(function(book){ %>
			<div>
				<b>Title: <%= book.title %> </b><br>
				<b>Authors: </b><%= book.authors %> <br>
				<b>Publisher: </b><%= book.publisher %> <br>
				<b>Published Date: </b><%= book.published_Date %> <br>
				<b>Industry Identifiers: </b><%= book.industryIdentifiers %> <br>
				<b>Page Count: </b><%= book.pageCount %> <br>
				<b>Categories: </b><%= book.categories %> <br>
				<b>Maturity Rating: </b><%= book.maturityRating %> <br>
				<b>Language: </b><%= book.language %> <br>
				<b>Id: </b><%= book.id %> <br>
				<b>Link: </b><%= book.link %> <br>
				<b>Thumbnail: </b><%= book.thmubnail %> <br>
				<b>Images: </b><%= book.images %> <br>
				<b>Description: </b></br><%= book.description %> <br>
			
			<hr>
			</div>
		<% }); %>
		<a href="/">
			<button type="button" class="btn btn-danger btn-sm" name="delete_item">Home</button>
		</a>
	</div>
</body>
</html>